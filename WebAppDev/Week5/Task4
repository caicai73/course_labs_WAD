b) select customers.name from stock, customers, orders 
   where stock.id = orders.ItemId 
    and Customers.id = orders.CustId 
    and stock.name = "Fred's Fries";

select customers.name from stock, customers, orders 
where stock.id=orders.ItemId 
    and Customers.id = orders.CustId 
    and stock.name like "%fred's fries%";

